# Rezdy API Configuration
# Get your API key from your Rezdy account settings
REZDY_API_KEY=your_rezdy_api_key_here
REZDY_API_URL=https://api.rezdy.com/v1

# Westpac Payment Configuration
WESTPAC_MERCHANT_ID=your_westpac_merchant_id
WESTPAC_SECRET_KEY=your_westpac_secret_key
WESTPAC_BASE_URL=https://quickstream.westpac.com.au
# For sandbox testing, use:
# WESTPAC_BASE_URL=https://quickstream-sandbox.westpac.com.au

# Next.js Configuration
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Optional: Database Configuration (if using database storage)
# DATABASE_URL=your_database_connection_string

# Redis Configuration (for enhanced caching)
# Leave empty to use memory-only caching
REDIS_URL=redis://localhost:6379
# For production Redis with authentication:
# REDIS_URL=redis://username:password@hostname:port

# Cache Configuration
CACHE_ENABLED=true
CACHE_COMPRESSION_ENABLED=true
CACHE_ANALYTICS_ENABLED=true 

# Stripe Payment Configuration
# Get your API keys from https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
# For webhooks (optional):
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here